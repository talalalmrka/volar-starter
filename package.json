    "release": "npm version patch -m \"chore: release v%s\" && npm run build && git push && git push --tags && npm publish"
  },
  "dependencies": {
    "vue": "latest"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "latest",
    "@vue/compiler-sfc": "latest",
    "typescript": "latest",
    "vite": "latest",
    "vite-plugin-inspect": "latest",
    "vite-plugin-vue-component-preview": "latest",
    "volar-service-prettyhtml": "latest",
    "vue-tsc": "latest"
  }
}
